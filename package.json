  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon server/main.js --ignore dist --ignore coverage --ignore tests --ignore src",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
