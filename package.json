{
  "name": "karaoke-forever",
  "version": "0.0.1",
  "description": "Self-hosted, multi-room, multi-user karaoke party system built on open web technologies",
  "private:": true,
  "main": "main.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "cross-env NODE_ENV=production npm run clean && npm run compile",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon server/main.js --ignore dist --ignore coverage --ignore tests --ignore src",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhj/karaoke-forever.git"
