{
  "name": "karaoke-forever",
  "version": "0.0.1",
  "description": "Karaoke LAN party system",
  "private:": true,
  "main": "./server/main.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development DEBUG=app:* nodemon --ignore dist --ignore coverage --ignore tests --ignore src",
    "compile": "npm run clean && cross-env NODE_ENV=production webpack",
    "serve": "cross-env NODE_ENV=production DEBUG=app:* node .",
    "start": "cross-env NODE_ENV=production DEBUG=app:* ELECTRON_ENABLE_LOGGING=true electron .",
    "dist": "npm run compile && cross-env DEBUG=electron-builder:* electron-builder",
    "build": "npm run dist -- --dir",
    "install-app-deps": "electron-builder install-app-deps"
  },
  "build": {
    "files": [
      "constants/**",
      "server/**",
      "dist/*",
      "project.config.js"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "msi"
    },
    "appId": "com.karaoke-forever.app",
    "productName": "Karaoke Forever"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhj/karaoke-forever.git"
  },
  "author": "Brandon H. Jones (http://karaoke-forever.com)",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.47",
    "bcrypt": "^2.0.1",
    "cdgraphics": "^1.0.2",
    "debug": "^3.1.0",
    "jsonwebtoken": "^8.2.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-range": "^0.3.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "mp3-duration": "^1.1.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "rc-slider": "^8.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-highlight-words": "^0.11.0",
    "react-measure": "^1.4.7",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-skylight": "^0.5.1",
    "react-swipeable": "^4.3.0",
    "react-transition-group": "^2.3.1",
    "react-virtualized": "^9.19.1",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-persist": "^5.9.1",
    "redux-responsive": "^4.3.7",
    "redux-throttle": "^0.1.1",
    "redux-thunk": "^2.0.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "screenfull": "^3.3.2",
    "socket.io": "^2.1.1",
    "sqlite": "^2.9.2",
    "squel": "^5.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-react-css-modules": "^3.4.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.2",
    "electron-builder": "^20.14.7",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "redbox-react": "^1.6.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4"
  }
}
